<p id="notice"><%= notice %></p>

<h1>Packages</h1>

<table class="ui celled table">
  <thead>
    <tr>
      <th>Code</th>
      <th>State</th>
      <th>Fragility</th>
      <th>Size</th>
      <th>Weight</th>
      <th>Value</th>
      <th>Conveyance</th>
      <th>Shipping date</th>
      <th>Delivery date</th>
      <th>Observations</th>
      <th>User</th>
      <th>Receiver</th>
      <th>Container</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @packages.each do |package| %>
      <tr>
        <td><%= package.code %></td>
        <td><%= package.state %></td>
        <td><%= package.fragility %></td>
        <td><%= package.size %></td>
        <td><%= package.weight %></td>
        <td><%= package.value %></td>
        <td><%= package.conveyance %></td>
        <td><%= package.shipping_date %></td>
        <td><%= package.delivery_date %></td>
        <td><%= package.observations %></td>
        <td><%= package.user %></td>
        <td><%= package.receiver %></td>
        <td><%= package.container %></td>
        <td><%= link_to 'Show', package %></td>
        <td><%= link_to 'Edit', edit_package_path(package) %></td>
        <td><%= link_to 'Destroy', package, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Package', new_package_path %>
