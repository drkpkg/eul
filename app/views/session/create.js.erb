<% if @status == false %>
  $('#message').append("<%= j render 'general/error_no_data' %>");
  $('.message .close')
    .on('click', function() {
      $(this)
        .closest('.message')
        .transition('fade')
      ;
    });
<% else %>
    $(location).attr('href', "<%= dashboard_url %>")
<% end %>
